@model HNS.Entities.Faq
@{
    ViewData["Title"] = "FAQ";
    ViewData["BreadCrumb"] = "게시판관리,FAQ";
    ViewData["CurrentMenu"] = "게시판관리";
    ViewData["CurrentMenuItem"] = "FAQ";
}

<div class="bbs-write">
    <dl>
        <dt><label for="title">ID</label></dt>
        <dd><input type="text" id="wr_subject" name="wr_subject" title="제목 입력" readonly value="@Model.Id"></dd>
    </dl>
    <dl>
        <dt>날짜</dt>
        <dd><input type="text" id="wr_1" name="wr_1" title="URL 입력" readonly value="@Model.m_date"></dd>
    </dl>
    <dl>
        <dt><label for="title">이메일</label></dt>
        <dd><input type="text" id="wr_subject" name="wr_subject" title="제목 입력" readonly value="@Model.m_email"></dd>
    </dl>
    <dl>
        <dt>키워드</dt>
        <dd><input type="text" id="wr_1" name="wr_1" title="URL 입력" readonly value="@Model.m_keyword"></dd>
    </dl>
    <dl>
        <dt>코멘트</dt>
        <dd><input type="text" id="wr_1" name="wr_1" title="코멘트" readonly value="@Model.m_comment"></dd>
    </dl>
    <dl>
        <dt><label for="title">조회수</label></dt>
        <dd><input type="text" id="wr_subject" name="wr_subject" title="제목 입력" readonly value="@Model.m_read"></dd>
    </dl>

    <dl>
        <dt>제목</dt>
        <dd><input type="text" id="wr_subject" name="wr_subject" title="제목 입력" readonly value="@Model.m_title"></dd>
    </dl>

    <dl class="content" style="min-height:200px;">
        <dt style="padding-top:0px;">본문</dt>
        <dd>
            @Html.Raw(Model.m_content)
        </dd>
    </dl>
</div>


<div class="btn_confirm01 btn_confirm">
    <a href="@Url.Action("FAQEdit", "Community", new { id=Model.Id })">
        편집
    </a>
    <a href="@Url.Action("FAQ", "Community")">
        다시 목록으로
    </a>
</div>

